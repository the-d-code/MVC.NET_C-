@model MvcApplication2.ServiceReference1.stud

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>stud</legend>

    <div class="display-label">course</div>
    <div class="display-field">@Model.course</div>

    <div class="display-label">rno</div>
